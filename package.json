    "add-liquidity": "npx hardhat run scripts/03-add-liquidity.js --network kovan",
    "remove-liquidity": "npx hardhat run scripts/04-remove-liquidity.js --network kovan",
    "deploy-swapper": "npx hardhat run scripts/05-deploy-swapper.js --network kovan",
    "swap-to-wbtc": "END_TOKEN=WBTC npx hardhat run scripts/06-test-swap.js --network kovan",
    "swap-to-tusd": "END_TOKEN=TUSD npx hardhat run scripts/06-test-swap.js --network kovan"
